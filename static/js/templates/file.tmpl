<a target="" href="#">
    <img src="../files/<%= id %>.png" alt="<%= filename %>.png" height="100" width="100">
</a>

<div class="btn-group btn-group-justified">
    <a class="btn btn-default" id="btnEditFile" data-id="<%= id %>.png"  href="/explore/<%= id %>.png" role="button">
        <span class="icon-edit">
    </a>
    <a class="btn btn-default" id="btnDeleteFile" data-id="<%= id %>.png" role="button">
        <span class="icon-trash">
    </a>
</div>

